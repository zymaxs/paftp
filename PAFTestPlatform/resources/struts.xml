<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.action.extension" value="action" />
	<constant name="struts.serve.static.browserCache" value="false" />
	<constant name="struts.devMode" value="true" />

    <package name="ajax" extends="json-default">
    <action name="querySutsAjax" method="querySut" class="com.paftp.action.ApplySutAction">
        <result type="json"></result>
    </action>
    <action name="queryRolesAjax" method="queryRoles" class="com.paftp.action.RoleAction">
         <result type="json">
         </result>
    </action>
    <action name="getUserInfoAjax" method="getRoles" class="com.paftp.action.UserProfileAction">
         <result type="json"></result>
    </action>
    <action name="initialTestsuites" method="initialTestsuites" class="com.paftp.action.TestsuiteAction">
         <result type="json">
         </result>
    </action>
        <action name="createTestsuite" method="createTestsuite" class="com.paftp.action.TestsuiteAction">
         <result type="json">
         </result>
    </action>
         <action name="deleteTestsuite" method="createTestsuite" class="com.paftp.action.TestsuiteAction">
         <result type="json">
         </result>
    </action>
         <action name="createTestcase" method="createTestsuite" class="com.paftp.action.TestsuiteAction">
         <result type="json">
         </result>
    </action>
         <action name="deleteTestcase" method="createTestsuite" class="com.paftp.action.TestsuiteAction">
         <result type="json">
         </result>
    </action>
    </package>

	<package name="s2sh" extends="struts-default">
		<action name="login" method="login" class="com.paftp.action.LoginAction">
			<result name="success" type="redirect">/index_1.jsp</result>
			<result name="logsuccess" >/index_1.jsp</result>
			<result name="error">/error.jsp</result>
			<result name="update" type="redirect">/update.jsp</result>
		</action>
		<action name="logout" method="logout" class="com.paftp.action.LoginAction">
			<result name="success" type="redirect">/login.jsp</result>
		</action>
		<action name="changepwd" method="changepwd" class="com.paftp.action.LoginAction">
			<result name="success" type="redirect">/index_1.jsp</result>
			<result name="error">/opr_error.jsp</result>
		</action>
			<action name="getbakpwd" method="getbakpwd" class="com.paftp.action.LoginAction">
			<result name="success">/changeinipwd.jsp</result>
			<result name="error" type="redirect">/opr_error.jsp</result>
		</action>
		<action name="register" method="register"
			class="com.paftp.action.RegisterAction">
			<result name="success">/changeinipwd.jsp</result>
			<result name="error">/register.jsp</result>
			<result name="exist">/error.jsp</result>
		</action>
		<action name="index" method="index" class="com.paftp.action.IndexAction">
			<result name="success">/home.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="updateuserinfo" method="updateUserInfo" class="com.paftp.action.UserProfileAction">
			<result name="success">/index_1.jsp</result>
			<result name="login">/login.jsp</result>
		</action>
		<action name="updatepassword" method="updatePassword" class="com.paftp.action.UserProfileAction">
			<result name="success">/index_1.jsp</result>
			<result name="login">/login.jsp</result>
			<result name="error">/opr_error.jsp</result>
		</action>
		  <action name="getuserinfo" method="getUserInfo" class="com.paftp.action.UserProfileAction">
        <result name="success">/userinfo.jsp</result>
    	 </action>
		<action name="applySut" method="applySut" class="com.paftp.action.ApplySutAction">
			<result name="success">/sutindex.jsp</result>
			<result name="error">/error.jsp</result>
			<result name="login">/login.jsp</result>
			<result name="exist">/error.jsp</result>
		</action>
		<action name="approveSut" method="approveSut" class="com.paftp.action.ApplySutAction">
			<result name="success">/sutindex.jsp</result>
			<result name="error">/error.jsp</result>
			<result name="login">/login.jsp</result>
		</action>
		<action name="initialSuts" method="initialSuts" class="com.paftp.action.ApplySutAction">
			<result name="success">/sutindex.jsp</result>
			<result name="login">/login.jsp</result>
		</action>
		<action name="initialSut" method="initialSut" class="com.paftp.action.ApplySutAction">
			<result name="success">/applysutinfo.jsp</result>
			<result name="login">/login.jsp</result>
		</action>
		<action name="updateSut" method="updateSut" class="com.paftp.action.ApplySutAction">
			<result name="success">/sutindex.jsp</result>
		</action>
		<action name="reguserinfo" method="userinfo" class="com.paftp.action.StaticAction">
			<result name="success">/register.jsp</result>
		</action>
		<action name="upuserinfo" method="userinfo" class="com.paftp.action.StaticAction">
			<result name="success">/updateuserinfo.jsp</result>
		</action>
			<action name="applysutgroup" method="sutgroup" class="com.paftp.action.StaticAction">
			<result name="success">/applysut.jsp</result>
		</action>
		<action name="showsutgroup" method="sutgroup" class="com.paftp.action.StaticAction">
			<result name="success">/applysutinfo.jsp</result>
		</action>
		<action name="casequeryGroups" method="queryGroups" class="com.paftp.action.ApplySutAction">
			<result name="success">/casemanagement.jsp</result>
		</action>
		<action name="rolequeryGroups" method="queryGroups" class="com.paftp.action.ApplySutAction">
			<result name="success">/rolemanagement.jsp</result>
		</action>
    	 <action name="initialroles" method="initialRoles" class="com.paftp.action.RoleAction">
        <result name="success">/roleindex.jsp</result>
    	 </action>
    	 <action name="sutinitialAddRelationship" method="initialAddRelationship" class="com.paftp.action.RoleAction">
        <result name="success">/sutroleuser.jsp</result>
        <result name="error">/error.jsp</result>
    	 </action>
    	 <action name="iniinitialAddRelationship" method="initialAddRelationship" class="com.paftp.action.RoleAction">
        <result name="success">/inimanager.jsp</result>
        <result name="error">/error.jsp</result>
    	 </action>
    	 <action name="sutaddRelationship" method="addRelationship" class="com.paftp.action.RoleAction">
        <result name="success">/dispatch.jsp</result>
        <result name="error">/error.jsp</result>
    	 </action>
    	 <action name="iniaddRelationship" method="addRelationship" class="com.paftp.action.RoleAction">
        <result name="success">/inimanager.jsp</result>
    	 </action>
    	 <action name="initialParameters" method="initialParameters" class="com.paftp.action.TestsuiteAction">
        <result name="success">/sutcase.jsp</result>
    	 </action>
    	 	 <action name="testDelete" method="testDelete" class="com.paftp.action.TestAction">
        <result name="success">/test.jsp</result>
    	 </action>
    	 

	</package>

</struts> 